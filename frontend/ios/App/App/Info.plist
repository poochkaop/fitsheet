<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CFBundleDevelopmentRegion</key>
	<string>en</string>
	<key>CFBundleDisplayName</key>
	<string>Fitsheet</string>
	<key>CFBundleExecutable</key>
	<string>$(EXECUTABLE_NAME)</string>
	<key>CFBundleIdentifier</key>
	<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleName</key>
	<string>$(PRODUCT_NAME)</string>
	<key>CFBundlePackageType</key>
	<string>APPL</string>
	<key>CFBundleShortVersionString</key>
	<string>$(MARKETING_VERSION)</string>
	<key>CFBundleURLTypes</key>
	<array>
		<dict>
			<key>CFBundleURLName</key>
			<string>com.fitsheet.app</string>
			<key>CFBundleURLSchemes</key>
			<array>
				<string>com.fitsheet.app</string>
			</array>
		</dict>
	</array>
	<key>CFBundleVersion</key>
	<string>$(CURRENT_PROJECT_VERSION)</string>
	<key>LSRequiresIPhoneOS</key>
	<true/>
	<key>NSAppTransportSecurity</key>
	<dict>
		<key>NSAllowsArbitraryLoads</key>
		<true/>
	</dict>
	<key>UILaunchStoryboardName</key>
	<string>LaunchScreen</string>
	<key>UIMainStoryboardFile</key>
	<string>Main</string>
	<key>UIRequiredDeviceCapabilities</key>
	<array>
		<string>armv7</string>
	</array>
	<key>UISupportedInterfaceOrientations</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationPortraitUpsideDown</string>
	</array>
	<key>UISupportedInterfaceOrientations~ipad</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationPortraitUpsideDown</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
	<key>UIViewControllerBasedStatusBarAppearance</key>
	<true/>
	
	<!-- Fitsheet Specific Settings -->
	<key>NSHealthShareUsageDescription</key>
	<string>Fitsheet would like to access your health data to provide personalized fitness recommendations and track your progress.</string>
	<key>NSHealthUpdateUsageDescription</key>
	<string>Fitsheet would like to update your health data with workout information to help track your fitness journey.</string>
	<key>NSMotionUsageDescription</key>
	<string>Fitsheet uses motion data to track your workouts and provide accurate fitness metrics.</string>
	<key>NSCameraUsageDescription</key>
	<string>Fitsheet would like to access your camera to take progress photos for your fitness journey.</string>
	<key>NSPhotoLibraryUsageDescription</key>
	<string>Fitsheet would like to access your photo library to save and view your progress photos.</string>
	
	<!-- Push Notifications -->
	<key>UIBackgroundModes</key>
	<array>
		<string>remote-notification</string>
	</array>
	
	<!-- App Icon -->
	<key>CFBundleIcons</key>
	<dict>
		<key>CFBundlePrimaryIcon</key>
		<dict>
			<key>CFBundleIconFiles</key>
			<array>
				<string>AppIcon</string>
			</array>
		</dict>
	</dict>
	
	<!-- App Category -->
	<key>LSApplicationCategoryType</key>
	<string>public.app-category.healthcare-fitness</string>
	
	<!-- Minimum iOS Version -->
	<key>MinimumOSVersion</key>
	<string>13.0</string>
</dict>
</plist>